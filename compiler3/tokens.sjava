(define-class LexedParsedToken (Token) public
	(endLine int public)
	(transformed Token public transient)
	((<init> line int) void public
		(super:<init> line)
	)
	((toksString l List{LexedParsedToken}) String
		(define s (StringBuffer))
		(forEach (tok i) l
			(s:append tok)
			(if (!= i (- (l:size) 1))
				(s:append " ")
			)
		)
		(s:toString)
	)
	((firstLine) int public
		this:line
	)
	((lastLine) int public
		this:endLine
	)
	((toStringParsed) String public
		(this:toString)
	)
	((<init>) void public
		(super:<init>)
	)
)

