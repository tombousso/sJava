(define-class Main () 'public
	((main args String[]) void 'public 'static
		(define i 0)
		(label a)
		(if (= i 1)
			(goto end)
		)
		(set i 1)
		(begin
			(define i 0)
			(label a)
			(if (!= i 5)
				(begin
					(System:out:println i)
